#include <bits/stdc++.h>
using namespace std;
#define int long long

void solve() {
    int n, m;
    cin >> n >> m;
    string x, s;
    cin >> x >> s;

    int count = 0;
    while (count <= 6) {
        if (x.find(s) != string::npos) {
            cout << count << "\n";
            return;
        }
        x += x; 
        count++;
    }
    cout << -1 << "\n";
}

int32_t main() {
    ios::sync_with_stdio(false);
    cin.tie(0);

    int t;
    cin >> t;
    while (t--) solve();
}
